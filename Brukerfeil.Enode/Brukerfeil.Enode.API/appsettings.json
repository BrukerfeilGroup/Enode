{
  "Appsettings": {
    "ConfigServer.BaseUrl": "%ConfigServer_BaseUrl%",
    "ConfigServer.Scopes": "FEATURES",
    "ConfigServer.Password": "%ConfigServer_Password%",
    "ConfigServer.LocalOverride": "false",
    "ConfigServer.RefreshFrequencySeconds": "10"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
